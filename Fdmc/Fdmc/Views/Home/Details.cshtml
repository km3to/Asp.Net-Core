@using Fdmc.Models.ViewModels

@model CatDetailsViewModel

@{
    this.ViewData["Title"] = "Details page";
}

<img src="@this.Model.ImageUrl" alt="image of a cat" />
<div>
    Name: @this.Model.Name
</div>
<div>
    Age: @this.Model.Age
</div>
<div>
    Breed: @this.Model.Breed
</div>
<div>
    Image: @this.Model.ImageUrl
</div>
